{
  "name": "aurora-app",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aurora-App/aurora-starter.git"
  },
  "scripts": {
    "format": "yarn prettier && yarn eslint",
    "eslint": "eslint --fix \"./app/**/**/**/**/*.{js,ts,jsx,tsx}\" --ext jsconfig.json",
    "prettier": "prettier --write \"./app/**/**/**/**/*.{js,ts,jsx,tsx}\"",
    "lint:css": "stylelint \"./app/**/**/**/**/*.{js,ts,jsx,tsx}\"",
    "db:generate": "prisma generate",
    "db:generate:acc": "prisma generate --accelerate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start",
    "deploy": "yarn db:generate:acc && yarn db:push && yarn build",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze-b": "cross-env BUNDLE_ANALYZE=both next build",
    "clean": "rimraf .next out"
  },
  "prisma": {
    "schema": "server/db/prisma/schema.prisma"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.14",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/lib-storage": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.3",
    "@prisma/client": "^5.8.0",
    "@prisma/extension-accelerate": "^0.6.2",
    "@t3-oss/env-nextjs": "^0.7.3",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "4.36.1",
    "@trpc/client": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "axios": "^1.6.5",
    "clsx": "^2.1.0",
    "cookie": "^0.6.0",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.18.0",
    "multer": "^1.4.5-lts.1",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "next-pwa": "^5.6.0",
    "nodemailer": "^6.9.8",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^9.1.3",
    "stripe": "^14.12.0",
    "superjson": "^2.2.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@emotion/babel-plugin": "^11.11.0",
    "@next/bundle-analyzer": "^14.0.4",
    "@types/cookie": "^0.6.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.0",
    "@types/nprogress": "^0.2.3",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18.2.47",
    "@types/tailwindcss": "^3.1.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "daisyui": "^4.6.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "file-loader": "^6.2.0",
    "glslify-import-loader": "^0.1.2",
    "glslify-loader": "^2.0.0",
    "next-compose-plugins": "^2.2.1",
    "postcss": "^8.4.33",
    "postcss-nesting": "^12.0.2",
    "prettier": "^3.2.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^5.8.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "url-loader": "^4.1.1",
    "yarn-upgrade-all": "^0.7.2"
  }
}
