{
  "name": "aurora-starter",
  "version": "2.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nexel-Lab/aurora-starter.git"
  },
  "bugs": {
    "url": "https://github.com/Nexel-Lab/aurora-starter/issues"
  },
  "homepage": "https://aurora.nexellab.com",
  "scripts": {
    "format": "pnpm prettier && pnpm eslint",
    "eslint": "eslint --fix \"./app/**/**/**/**/*.{js,ts,jsx,tsx}\" --ext jsconfig.json",
    "prettier": "prettier --write \"./app/**/**/**/**/*.{js,ts,jsx,tsx}\"",
    "lint:css": "stylelint \"./app/**/**/**/**/*.{js,ts,jsx,tsx}\"",
    "db:generate": "prisma generate",
    "db:generate:acc": "prisma generate --accelerate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db": "pnpm db:generate && pnpm db:push",
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start",
    "deploy": "pnpm db && pnpm build",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze-b": "cross-env BUNDLE_ANALYZE=both next build",
    "clean": "rimraf .next out"
  },
  "prisma": {
    "schema": "server/db/prisma/schema.prisma"
  },
  "yarn-upgrade-all": {
    "ignore": [
      "@tanstack/react-query"
    ]
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.6.0",
    "@aws-sdk/client-s3": "^3.554.0",
    "@aws-sdk/lib-storage": "^3.554.0",
